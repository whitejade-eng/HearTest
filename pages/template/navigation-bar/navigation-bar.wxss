/* pages/template/navigation-bar/navigation-bar.wxss */


.nav-btn image{
  width: 24px;
  height: 24px;
}

.nav-bar{ position: fixed; width: 100%; top: 0; color: white; background-color: white; z-index: 1;}
.nav-bar .nav-btn{ 
  position: absolute;
  border-radius: 50px; 
  padding-left: 14px;
  background-color: transparent;
  height: 30px;
  width: 30px;
  padding: 0;
}

.nav-bar .nav-text{ 
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: transparent;

}